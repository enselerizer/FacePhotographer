{
  "name": "face-photographer",
  "displayName": "Face Photographer",
  "version": "1.6.0",
  "author": {
    "name": "Александр Дергунов",
    "email": "enselerizer@gmail.com"
  },
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href ./ && tsc --p electron && electron .",
    "start-nobuild": "tsc --p electron && electron .",
    "rebuild": "electron-rebuild",
    "dist": "electron-builder",
    "build": "ng build --base-href ./ && tsc --p electron && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "enselerizer.face_photographer",
    "productName": "Face Photographer",
    "asar": false,
    "buildDependenciesFromSource": true,
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      {
        "from": "dist/FacePhotographer",
        "to": "dist/FacePhotographer",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular/animations": "~11.2.7",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@covalent/core": "^3.1.0",
    "@covalent/dynamic-forms": "^3.1.0",
    "@covalent/echarts": "^3.1.0",
    "@covalent/highlight": "^3.1.0",
    "@covalent/http": "^3.1.0",
    "@covalent/markdown": "^3.1.0",
    "@types/echarts": "^4.9.6",
    "@types/zrender": "^4.0.0",
    "angular-cropperjs": "^1.0.2",
    "cropperjs": "^1.5.11",
    "data-uri-to-buffer": "^3.0.1",
    "echarts": "^5.0.2",
    "electron-packager": "^15.2.0",
    "ngx-webcam": "^0.3.2",
    "node-sass": "^4.14.1",
    "rxjs": "~6.6.0",
    "serialport": "^8.0.8",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "zrender": "^5.0.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^11.4.1",
    "electron-builder": "^22.10.5",
    "electron-rebuild": "^2.3.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
